  <!--Logging-->
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="TraceAppender" />
      <appender-ref ref="RollingFile" />
    </root>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFile" type="log4net.Appender.FileAppender">
      <file value="Log\PLMPackLib.txt" />
      <appendToFile value="false" />
      <threshold value="INFO" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="*%-10level %-30date %-25logger %message %newline" />
      </layout>
    </appender>
    <system.diagnostics>
      <trace autoflush="true" indentsize="3" />
    </system.diagnostics>
  </log4net>
  <!--Set file repository path here-->
  <CustomSection databaseFile="K:\GitHub\PLMPackLib\PicSharpDb\PicParamData\Database\PicParam.db" thumbnailsPath="K:\GitHub\PLMPackLib\Pic.DAL\Thumbnails\" />
